<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">
    <uses-permission android:name="android.permission.INTERNET"/>
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@drawable/learn"
        android:label="Elearning"
        android:roundIcon="@drawable/learn"
        android:supportsRtl="true"
        android:theme="@style/Theme.Elearning"
        tools:targetApi="31">
        <activity
            android:name=".contentmain"
            android:exported="true"
            android:label="@string/app_name"
            android:theme="@style/Theme.Elearning">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".MainActivity"/>
        <activity android:name=".register"/>
        <activity android:name=".login"/>
        <activity android:name=".student"/>
        <activity android:name=".instructor"/>
        <activity android:name=".addcourse"/>
        <activity android:name=".coursedetails"/>
        <activity android:name=".quiz"/>
        <activity android:name=".addquestion"/>
        <activity android:name=".studentquiz"/>
        <activity android:name=".student123"/>
    </application>

</manifest>